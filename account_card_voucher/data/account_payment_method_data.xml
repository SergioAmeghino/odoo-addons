<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="account_payment_method_received_third_voucher" model="account.payment.method">
        <field name="name">Received Third Voucher</field>
        <field name="code">received_third_voucher</field>
        <field name="payment_type">inbound</field>
    </record>

    <record id="account_payment_method_delivered_third_voucher" model="account.payment.method">
        <field name="name">Delivered Third Voucher</field>
        <field name="code">delivered_third_voucher</field>
        <field name="payment_type">outbound</field>
    </record>

    <record id="account_payment_method_issue_voucher" model="account.payment.method">
        <field name="name">Issue Voucher</field>
        <field name="code">issue_voucher</field>
        <field name="payment_type">outbound</field>
    </record>

    <!--function model="account.journal" name="_enable_issue_voucher_on_bank_journals"/-->

</odoo>
